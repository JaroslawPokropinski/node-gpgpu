{
  "main": "./dist/binding.js",
  "types": "./dist/binding.d.ts",
  "scripts": {
    "pretest": "tsc",
    "test": "ts-mocha 'test/**/*.spec.?s' --reporter list",
    "build": "tsc",
    "watch": "tsc -w",
    "install": "cmake-js compile",
    "rebuild": "node-gyp rebuild",
    "prebuild": "prebuild --runtime napi --all --strip --verbose",
    "upload": "prebuild --runtime napi --upload ${GITHUB_TOKEN}",
    "prepare": "husky install"
  },
  "gypfile": true,
  "name": "gpgpu",
  "version": "0.0.3",
  "author": "JaroslawPokropinski",
  "license": "ISC",
  "description": "Node.js library for gpu acceleration using pure javascript",
  "dependencies": {
    "acorn": "^7.4.0",
    "esprima": "^4.0.1",
    "node-addon-api": "^3.0.0",
    "prebuild": "^10.0.0",
    "prebuild-install": "^5.3.5",
    "recast": "^0.19.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/esprima": "^4.0.2",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "chai": "^4.2.0",
    "cmake-js": "^6.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mocha": "^8.2.1",
    "mrm": "^3.0.2",
    "mrm-task-lint-staged": "^6.0.0",
    "prettier": "^2.0.5",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.0.0"
  },
  "lint-staged": {
    "*.js": "prettier --write .",
    "*.ts": "prettier --write ."
  }
}
